<Application x:Class="ThemedDialog.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="AppWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/VsBrushes.xaml" />
                <ResourceDictionary Source="Resources/CommonControlsColors.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ResourceDictionary x:Key="ThemedDialogDefaultStylesKey" Source="Resources/ThemedDialogDefaultStyles.xaml" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
